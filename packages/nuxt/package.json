{
    "name": "@soave/nuxt-ui",
    "version": "0.2.0",
    "description": "Nuxt module for soave UI - Deep Nuxt integration with SSR, i18n, and routing support",
    "author": "Arata Ouchi (Original SIN Architecture)",
    "license": "MIT",
    "type": "module",
    "exports": {
        ".": {
            "types": "./dist/types.d.ts",
            "import": "./dist/module.mjs",
            "require": "./dist/module.cjs"
        }
    },
    "main": "./dist/module.cjs",
    "module": "./dist/module.mjs",
    "types": "./dist/types.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "nuxt-module-build build",
        "dev": "nuxt-module-build build --stub",
        "typecheck": "vue-tsc --noEmit"
    },
    "keywords": [
        "nuxt",
        "nuxt-module",
        "vue",
        "ui",
        "components",
        "soave"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/artouc/soaveui"
    },
    "homepage": "https://soave.io",
    "dependencies": {
        "@nuxt/kit": "^3.14.0"
    },
    "devDependencies": {
        "@nuxt/module-builder": "^0.8.0",
        "@nuxt/schema": "^3.14.0",
        "nuxt": "^3.14.0",
        "typescript": "^5.7.0",
        "vue-tsc": "^2.2.0"
    },
    "peerDependencies": {
        "nuxt": "^3.0.0 || ^4.0.0"
    }
}
