{
    "name": "@soave/ui",
    "version": "0.3.1",
    "description": "soave UI - Composable-First UI Framework for Vue 3 / Nuxt 4",
    "author": "Arata Ouchi (Original SIN Architecture)",
    "license": "MIT",
    "homepage": "https://soave.io",
    "repository": {
        "type": "git",
        "url": "https://github.com/artouc/soaveui"
    },
    "type": "module",
    "main": "./dist/index.mjs",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.mjs",
            "types": "./dist/index.d.ts"
        },
        "./composables": {
            "import": "./dist/composables/index.mjs",
            "types": "./dist/composables/index.d.ts"
        },
        "./adapters": {
            "import": "./dist/adapters/index.mjs",
            "types": "./dist/adapters/index.d.ts"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "unbuild",
        "dev": "unbuild --stub",
        "typecheck": "vue-tsc --noEmit"
    },
    "peerDependencies": {
        "vue": "^3.4.0"
    },
    "dependencies": {
        "clsx": "^2.1.0",
        "tailwind-merge": "^2.2.0",
        "zod": "^3.22.0"
    },
    "devDependencies": {
        "@vue/tsconfig": "^0.5.0",
        "typescript": "^5.4.0",
        "unbuild": "^2.0.0",
        "vue": "^3.4.0",
        "vue-tsc": "^2.2.12"
    }
}
