{
    "name": "@soave/tailwind",
    "version": "0.3.0",
    "description": "Tailwind CSS styled components for soave UI",
    "author": "Arata Ouchi (Original SIN Architecture)",
    "license": "MIT",
    "homepage": "https://soave.io",
    "repository": {
        "type": "git",
        "url": "https://github.com/artouc/soaveui"
    },
    "type": "module",
    "main": "./dist/index.mjs",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.mjs",
            "types": "./dist/index.d.ts"
        },
        "./components/*": {
            "import": "./dist/components/*.mjs",
            "types": "./dist/components/*.d.ts"
        },
        "./tailwind.config": {
            "import": "./tailwind.config.js",
            "require": "./tailwind.config.js"
        }
    },
    "files": [
        "dist",
        "tailwind.config.js"
    ],
    "scripts": {
        "build": "vite build",
        "dev": "vite build --watch",
        "typecheck": "vue-tsc --noEmit"
    },
    "dependencies": {
        "@soave/ui": "^0.5.0"
    },
    "peerDependencies": {
        "vue": "^3.4.0",
        "tailwindcss": "^3.0.0"
    },
    "devDependencies": {
        "@vitejs/plugin-vue": "^6.0.0",
        "@vue/tsconfig": "^0.5.0",
        "typescript": "^5.4.0",
        "vite": "^6.0.0",
        "vite-plugin-dts": "^4.3.0",
        "vue": "^3.4.0",
        "vue-tsc": "^2.2.12",
        "tailwindcss": "^3.4.0"
    }
}
